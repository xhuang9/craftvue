{
  "require": {
    "barrelstrength/sprout-fields": "3.6.1",
    "craftcms/cms": "3.4.6.1",
    "nystudio107/craft-twigpack": "1.1.14",
    "verbb/field-manager": "2.1.1",
    "verbb/super-table": "2.4.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
