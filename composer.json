{
  "require": {
    "barrelstrength/sprout-fields": "3.8.3",
    "craftcms/cms": "3.4.21",
    "craftcms/commerce": "3.1.5",
    "craftcms/commerce-stripe": "2.3.0",
    "craftcms/contact-form": "2.2.7",
    "craftcms/feed-me": "4.2.3",
    "craftcms/guest-entries": "2.3.0",
    "craftcms/redactor": "2.6.1",
    "doublesecretagency/craft-cpcss": "2.2.1",
    "doublesecretagency/craft-cpjs": "2.2.1",
    "ether/logs": "3.0.3",
    "marionnewlevant/twig-perversion": "2.1.0",
    "nystudio107/craft-seomatic": "3.3.5",
    "nystudio107/craft-twigpack": "1.2.2",
    "putyourlightson/craft-dashboard-begone": "1.0.1",
    "verbb/field-manager": "2.1.6",
    "verbb/super-table": "2.4.9",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
